package seed.jsp;

public class EmpBean {

	//private Bean Properties
	private String username;
	private String pass;
	private String dbDriver,dbUrl,dbUser,dbPass;
	
	//Constructors
	public EmpBean(){
		super();
	}
	public EmpBean(String username, String pass, String dbDriver, String dbUrl, String dbUser, String dbPass) {
		super();
		this.username = username;
		this.pass = pass;
		this.dbDriver = dbDriver;
		this.dbUrl = dbUrl;
		this.dbUser = dbUser;
		this.dbPass = dbPass;
	}
	
	
}
